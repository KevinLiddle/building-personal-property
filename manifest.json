{
  "manifest_version" : 2,
  "name" : "building-personal-property",
  "version" : "0.0.1",
  "description" : "Warn user when typing \"Building personal property\"",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["building-personal-property.js"]
    }
  ],
  "offline_enabled" : true,
  "permissions": ["tabs"]
}
